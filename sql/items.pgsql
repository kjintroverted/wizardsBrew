INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Pearl', 'Treasure', 100, null, 'false', 'Common', null, null, array[row('Description', array['An opaque lustrous white, yellow, or pink gemstone.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Pearl of Power', 'Wondrous Item', null, null, 'by a Spellcaster', 'Uncommon', null, null, array[row('Description', array['While this pearl is on your person, you can use an action to speak its command word and regain one expended spell slot. If the expended slot was of 4th level or higher, the new slot is 3rd level. Once you have used the pearl, it can''t be used again until the next dawn.'])::section]);
