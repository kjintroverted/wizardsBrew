INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Burnt Othur Fumes (Inhaled)', 'Adventuring Gear', 500, null, 'false', 'Common', null, null, array[row('Description', array['A creature subjected to this poison must succeed on a DC 13 Constitution saving throw or take 10 ({@dice 3d6}) poison damage, and must repeat the saving throw at the start of each of its turns. On each successive failed save, the character takes 3 ({@dice 1d6}) poison damage. After three successful saves, the poison ends.'])::section]);
