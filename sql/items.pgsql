INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Mace of Disruption', 'Melee Weapon', null, 4, 'true', 'Rare', null, null, array[row('Description', array['When you hit a fiend or an undead with this magic weapon, that creature takes an extra {@dice 2d6} radiant damage. If the target has 25 hit points or fewer after taking this damage, it must succeed on a DC 15 Wisdom saving throw or be destroyed. On a successful save, the creature becomes {@condition frightened} of you until the end of your next turn.','While you hold this weapon, it sheds bright light in a 20-foot radius and dim light for an additional 20 feet.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Mace of Smiting', 'Melee Weapon', null, 4, 'false', 'Rare', null, null, array[row('Description', array['You gain a +1 bonus to attack and damage rolls made with this magic weapon. The bonus increases to +3 when you use the mace to attack a construct.','When you roll a 20 on an attack roll made with this weapon, the target takes an extra 7 bludgeoning damage, or an extra 14 bludgeoning damage if it''s a construct. If a construct has 25 hit points or fewer after taking this damage, it is destroyed.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Mace of Terror', 'Melee Weapon', null, 4, 'true', 'Rare', null, null, array[row('Description', array['This magic weapon has 3 charges. While holding it, you can use an action and expend 1 charge to release a wave of terror. Each creature of your choice in a 30-foot radius extending from you must succeed on a DC 15 Wisdom saving throw or become {@condition frightened} of you for 1 minute. While it is {@condition frightened} in this way, a creature must spend its turns trying to move as far away from you as it can, and it can''t willingly move to a space within 30 feet of you. It also can''t take reactions. For its action it can use only the {@action Dash} action or try to escape from an effect that prevents it from moving. If it has nowhere it can move, the creature can use the {@action Dodge} action. At the end of each of its turns, a creature can repeat the saving throw, ending the effect on itself on a success.','The mace regains {@dice 1d3} expended charges daily at dawn.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Magnifying Glass', 'Adventuring Gear', 100, null, 'false', 'Common', null, null, array[row('Description', array['This lens allows a closer look at small objects. It is also useful as a substitute for flint and steel when starting fires. Lighting a fire with a magnifying glass requires light as bright as sunlight to focus, tinder to ignite, and about 5 minutes for the fire to ignite. A magnifying glass grants advantage on any ability check made to appraise or inspect an item that is small or highly detailed.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Malachite', 'Treasure', 10, null, 'false', 'Common', null, null, array[row('Description', array['An opaque striated light and dark green gemstone.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Malice (Inhaled)', 'Adventuring Gear', 250, null, 'false', 'Common', null, null, array[row('Description', array['A creature subjected to this poison must succeed on a DC 15 Constitution saving throw or become {@condition poisoned} for 1 hour. The {@condition poisoned} creature is {@condition blinded}.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Manacles', 'Adventuring Gear', 2, 6, 'false', 'Common', null, null, array[row('Description', array['These metal restraints can bind a Small or Medium creature. Escaping the manacles requires a successful DC 20 Dexterity check. Breaking them requires a successful DC 20 Strength check. Each set of manacles comes with one key. Without the key, a creature proficient with thieves'' tools can pick the manacles'' lock with a successful DC 15 Dexterity check. Manacles have 15 hit points.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Mantle of Spell Resistance', 'Wondrous Item', null, null, 'true', 'Rare', null, null, array[row('Description', array['You have advantage on saving throws against spells while you wear this cloak.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Manual of Bodily Health', 'Wondrous Item', null, 5, 'false', 'Very Rare', null, null, array[row('Description', array['This book contains health and diet tips, and its words are charged with magic. If you spend 48 hours over a period of 6 days or fewer studying the book''s contents and practicing its guidelines, your Constitution score increases by 2, as does your maximum for that score. The manual then loses its magic, but regains it in a century.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Manual of Clay Golems', 'Wondrous Item', null, 5, 'false', 'Very Rare', null, null, array[row('Description', array['This tome contains information and incantations necessary to make a particular type of golem. The DM chooses the type or determines it randomly. To decipher and use the manual, you must be a spellcaster with at least two 5th-level spell slots. A creature that can''t use a manual of golems and attempts to read it takes {@dice 6d6} psychic damage.','To create a {@creature clay golem}, you must spend 30 days, working without interruption with the manual at hand and resting no more than 8 hours per day. You must also pay 65,000 gp to purchase supplies. Once you finish creating the golem, the book is consumed in eldritch flames. The golem becomes animate when the ashes of the manual are sprinkled on it. It is under your control, and it understands and obeys your spoken commands.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Manual of Flesh Golems', 'Wondrous Item', null, 5, 'false', 'Very Rare', null, null, array[row('Description', array['This tome contains information and incantations necessary to make a particular type of golem. The DM chooses the type or determines it randomly. To decipher and use the manual, you must be a spellcaster with at least two 5th-level spell slots. A creature that can''t use a manual of golems and attempts to read it takes {@dice 6d6} psychic damage.','To create a {@creature flesh golem}, you must spend 60 days, working without interruption with the manual at hand and resting no more than 8 hours per day. You must also pay 50,000 gp to purchase supplies. Once you finish creating the golem, the book is consumed in eldritch flames. The golem becomes animate when the ashes of the manual are sprinkled on it. It is under your control, and it understands and obeys your spoken commands.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Manual of Gainful Exercise', 'Wondrous Item', null, 5, 'false', 'Very Rare', null, null, array[row('Description', array['This book describes fitness exercises, and its words are charged with magic. If you spend 48 hours over a period of 6 days or fewer studying the book''s contents and practicing its guidelines, your Strength score increases by 2, as does your maximum for that score. The manual then loses its magic, but regains it in a century.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Manual of Iron Golems', 'Wondrous Item', null, 5, 'false', 'Very Rare', null, null, array[row('Description', array['This tome contains information and incantations necessary to make a particular type of golem. The DM chooses the type or determines it randomly. To decipher and use the manual, you must be a spellcaster with at least two 5th-level spell slots. A creature that can''t use a manual of golems and attempts to read it takes {@dice 6d6} psychic damage.','To create an {@creature iron golem}, you must spend 120 days, working without interruption with the manual at hand and resting no more than 8 hours per day. You must also pay 100,000 gp to purchase supplies. Once you finish creating the golem, the book is consumed in eldritch flames. The golem becomes animate when the ashes of the manual are sprinkled on it. It is under your control, and it understands and obeys your spoken commands.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Manual of Quickness of Action', 'Wondrous Item', null, 5, 'false', 'Very Rare', null, null, array[row('Description', array['This book contains coordination and balance exercises, and its words are charged with magic. If you spend 48 hours over a period of 6 days or fewer studying the book''s contents and practicing its guidelines, your Dexterity score increases by 2, as does your maximum for that score. The manual then loses its magic, but regains it in a century.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Manual of Stone Golems', 'Wondrous Item', null, 5, 'false', 'Very Rare', null, null, array[row('Description', array['This tome contains information and incantations necessary to make a particular type of golem. The DM chooses the type or determines it randomly. To decipher and use the manual, you must be a spellcaster with at least two 5th-level spell slots. A creature that can''t use a manual of golems and attempts to read it takes {@dice 6d6} psychic damage.','To create a {@creature stone golem}, you must spend 90 days, working without interruption with the manual at hand and resting no more than 8 hours per day. You must also pay 80,000 gp to purchase supplies. Once you finish creating the golem, the book is consumed in eldritch flames. The golem becomes animate when the ashes of the manual are sprinkled on it. It is under your control, and it understands and obeys your spoken commands.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Map or Scroll Case', 'Adventuring Gear', 1, 1, 'false', 'Common', null, null, array[row('Description', array['This cylindrical leather case can hold up to ten rolled-up sheets of paper or five rolled-up sheets of parchment.'])::section]);
