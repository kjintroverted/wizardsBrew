INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Component Pouch', 'Adventuring Gear', 25, 2, 'false', 'Common', null, null, array[row('Description', array['A component pouch is a small, watertight leather belt pouch that has compartments to hold all the material components and other special items you need to cast your spells, except for those components that have a specific cost (as indicated in a spell''s description).'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Cook''s Utensils', 'Artisan Tools', 1, 8, 'false', 'Common', null, null, null);
