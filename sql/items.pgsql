INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Ring Mail', 'Heavy Armor', 30, 40, 'false', 'Common', null, 14, array[row('Description', array['This armor is leather armor with heavy rings sewn into it. The rings help reinforce the armor against blows from swords and axes. Ring mail is inferior to chain mail, and it''s usually worn only by those who can''t afford better armor.'])::section]);
