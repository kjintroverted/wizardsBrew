INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Acid (vial)', 'Adventuring Gear', 25, 1, 'false', 'Common', null, null, array[row('Description', array['As an action, you can splash the contents of this vial onto a creature within 5 feet of you or throw the vial up to 20 feet, shattering it on impact. In either case, make a ranged attack against a creature or object, treating the acid as an improvised weapon. On a hit, the target takes {@dice 2d6} acid damage.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Airship', 'Air Vehicle', 20000, null, 'false', 'Common', null, null, null);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Alchemist''s Fire (flask)', 'Adventuring Gear', 50, 1, 'false', 'Common', null, null, array[row('Description', array['This sticky, adhesive fluid ignites when exposed to air. As an action, you can throw this flask up to 20 feet, shattering it on impact. Make a ranged attack against a creature or object, treating the alchemist''s fire as an improvised weapon. On a hit, the target takes {@dice 1d4} fire damage at the start of each of its turns. A creature can end this damage by using its action to make a DC 10 Dexterity check to extinguish the flames.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Alchemist''s Supplies', 'Artisan Tools', 50, 8, 'false', 'Common', null, null, null);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Alchemy Jug', 'Wondrous Item', null, 12, 'false', 'Uncommon', null, null, array[row('| Liquid | Max Amount |', array['| Acid | 8 ounces |','| Basic poison | ½ ounce |','| Beer | 4 gallons |','| Honey | 1 gallon |','| Mayonnaise | 2 gallons |','| Oil | 1 quart |','| Vinegar | 2 gallons |','| Water, fresh | 8 gallons |','| Water, salt | 12 gallons |','| Wine | 1 gallon |'])::section,row('Description', array['This ceramic jug appears to be able to hold a gallon of liquid and weighs 12 pounds whether full or empty. Sloshing sounds can be heard from within the jug when it is shaken, even if the jug is empty.','You can use an action and name one liquid from the table below to cause the jug to produce the chosen liquid. Afterward, you can uncork the jug as an action and pour that liquid out, up to 2 gallons per minute. The maximum amount of liquid the jug can produce depends on the liquid you named.','Once the jug starts producing a liquid, it can''t produce a different one, or more of one that has reached its maximum, until the next dawn.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Alexandrite', 'Treasure', 500, null, 'false', 'Common', null, null, array[row('Description', array['A transparent dark green gemstone.'])::section]);
