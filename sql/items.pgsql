INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Pale Tincture (Ingested)', 'Adventuring Gear', 250, null, 'false', 'Common', null, null, array[row('Description', array['A creature subjected to this poison must succeed on a DC 16 Constitution saving throw or take 3 ({@dice 1d6}) poison damage and become {@condition poisoned}. The {@condition poisoned} creature must repeat the saving throw every 24 hours, taking 3 ({@dice 1d6}) poison damage on a failed save. Until this poison ends, the damage the poison deals can''t be healed by any means. After seven successful saving throws, the effect ends and the creature can heal normally.'])::section]);
