INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Pike', 'Melee Weapon', 5, 18, 'false', 'Common', row('Martial', null, 'piercing')::weapon_info, null, array[row('Heavy', array['Small creatures have disadvantage on attack rolls with heavy weapons. A heavy weapon''s size and bulk make it too large for a Small creature to use effectively.'])::section,row('Reach', array['This weapon adds 5 feet to your reach when you attack with it. This property also determines your reach for opportunity attacks with a reach weapon.'])::section,row('Two-Handed', array['This weapon requires two hands to use. This property is relevant only when you attack with the weapon, not when you simply hold it.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Pistol', 'Ranged Weapon', 250, 3, 'false', 'Common', row('Martial', null, 'piercing')::weapon_info, null, array[row('Ammunition', array['You can use a weapon that has the ammunition property to make a ranged attack only if you have ammunition to fire from the weapon. Each time you attack with the weapon, you expend one piece of ammunition. Drawing the ammunition from a quiver, case, or other container is part of the attack. Loading a one-handed weapon requires a free hand. The ammunition of a firearm is destroyed upon use.','If you use a weapon that has the ammunition property to make a melee attack, you treat the weapon as an improvised weapon. A sling must be loaded to deal any damage when used in this way.'])::section,row('Loading', array['Because of the time required to load this weapon, you can fire only one piece of ammunition from it when you use an action, bonus action, or reaction to fire it, regardless of the number of attacks you can normally make.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Plate Armor', 'Heavy Armor', 1500, 65, 'false', 'Common', null, 18, array[row('Description', array['Plate consists of shaped, interlocking metal plates to cover the entire body. A suit of plate includes gauntlets, heavy leather boots, a visored helmet, and thick layers of padding underneath the armor. Buckles and straps distribute the weight over the body.'])::section]);
