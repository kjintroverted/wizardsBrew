INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Disguise Kit', 'Tool', 25, 3, 'false', 'Common', null, null, array[row('Description', array['This pouch of cosmetics, hair dye, and small props lets you create disguises that change your physical appearance. Proficiency with this kit lets you add your proficiency bonus to any ability checks you make to create a visual disguise.'])::section]);
