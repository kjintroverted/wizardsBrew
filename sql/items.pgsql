INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Two-Person Tent', 'Adventuring Gear', 2, 20, 'false', 'Common', null, null, array[row('Description', array['A simple and portable canvas shelter, a tent sleeps two.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Universal Solvent', 'Wondrous Item', null, null, 'false', 'Legendary', null, null, array[row('Description', array['This tube holds milky liquid with a strong alcohol smell. You can use an action to pour the contents of the tube onto a surface within reach. The liquid instantly dissolves up to 1 square foot of adhesive it touches, including {@item sovereign glue}.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Vial', 'Adventuring Gear', 1, null, 'false', 'Common', null, null, array[row('Description', array['A vial can hold up to 4 ounces of liquid.'])::section]);
