INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Antimatter Rifle', 'Ranged Weapon', null, 10, 'false', 'Common', row('Martial', null, 'necrotic')::weapon_info, null, array[row('Ammunition', array['You can use a weapon that has the ammunition property to make a ranged attack only if you have ammunition to fire from the weapon. Each time you attack with the weapon, you expend one piece of ammunition. Drawing the ammunition from a quiver, case, or other container is part of the attack. Loading a one-handed weapon requires a free hand. The ammunition of a firearm is destroyed upon use.','If you use a weapon that has the ammunition property to make a melee attack, you treat the weapon as an improvised weapon. A sling must be loaded to deal any damage when used in this way.'])::section,row('Reload', array['A limited number of shots can be made with a weapon that has the reload property. A character must then reload it using an action or a bonus action (the character''s choice).'])::section,row('Two-Handed', array['This weapon requires two hands to use. This property is relevant only when you attack with the weapon, not when you simply hold it.'])::section]);
