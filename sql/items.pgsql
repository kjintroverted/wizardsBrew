INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Keelboat', 'Water Vehicle', 3000, null, 'false', 'Common', null, null, array[row('Description', array['Keelboats and rowboats are used on lakes and rivers. If going downstream, add the speed of the current (typically 3 miles per hour) to the speed of the vehicle. These vehicles can''t be rowed against any significant current, but they can be pulled upstream by draft animals on the shores.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Keoghtom''s Ointment', 'Wondrous Item', null, null, 'false', 'Uncommon', null, null, array[row('Description', array['This glass jar, 3 inches in diameter, contains {@dice 1d4+1} doses of a thick mixture that smells faintly of aloe. The jar and its contents weigh ½ pound.','As an action, one dose of the ointment can be swallowed or applied to the skin. The creature that receives it regains {@dice 2d8+2} hit points, ceases to be {@condition poisoned}, and is cured of any disease.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Ladder (10-foot)', 'Adventuring Gear', 0.1, 25, 'false', 'Common', null, null, array[row('Description', array['Ladder (10-foot)'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Lamp', 'Adventuring Gear', 0.5, 1, 'false', 'Common', null, null, array[row('Description', array['A lamp casts bright light in a 15-foot radius and dim light for an additional 30 feet. Once lit, it burns for 6 hours on a flask (1 pint) of oil.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Lantern of Revealing', 'Wondrous Item', null, 2, 'false', 'Uncommon', null, null, array[row('Description', array['While lit, this {@item hooded lantern|phb} burns for 6 hours on 1 pint of oil, shedding bright light in a 30-foot radius and dim light for an additional 30 feet. {@condition Invisible} creatures and objects are visible as long as they are in the lantern''s bright light. You can use an action to lower the hood, reducing the light to dim light in a 5-foot radius.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Lapis Lazuli', 'Treasure', 10, null, 'false', 'Common', null, null, array[row('Description', array['An opaque light and dark blue with yellow flecks gemstone.'])::section]);
