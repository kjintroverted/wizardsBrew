INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Belt of Dwarvenkind', 'Wondrous Item', null, null, 'true', 'Rare', null, null, array[row('Description', array['While wearing this belt, you gain the following benefits:','In addition, while attuned to the belt, you have a {@chance 50} chance each day at dawn of growing a full beard if you''re capable of growing one, or a visibly thicker beard if you already have one.','If you aren''t a dwarf, you gain the following additional benefits while wearing the belt:'])::section]);
