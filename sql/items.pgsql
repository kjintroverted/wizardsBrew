INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Gem of Brightness', 'Wondrous Item', null, 1, 'false', 'Uncommon', null, null, array[row('Description', array['This prism has 50 charges. While you are holding it, you can use an action to speak one of three command words to cause one of the following effects:','When all of the gem''s charges are expended, the gem becomes a nonmagical jewel worth 50 gp.'])::section]);
INSERT INTO items (name, type, cost, weight, attune, rarity, weapon, armor_class, info) VALUES ('Gem of Seeing', 'Wondrous Item', null, 1, 'true', 'Rare', null, null, array[row('Description', array['This gem has 3 charges. As an action, you can speak the gem''s command word and expend 1 charge. For the next 10 minutes, you have truesight out to 120 feet when you peer through the gem.','The gem regains {@dice 1d3} expended charges daily at dawn.'])::section]);
